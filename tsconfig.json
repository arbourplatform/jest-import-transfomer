{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "noEmit": false,
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "jsx": "react",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceRoot": "src",
    "lib": ["ESNext"],
    "outDir": "./dist"
  },
  "include": ["./src/**/*"]
}
